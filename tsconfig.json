{
  "extends": "@elzup/tsconfig/tsconfig.node.json",
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "noUncheckedIndexedAccess": false,
    "noPropertyAccessFromIndexSignature": false,
    "outDir": "lib",
    "baseUrl": ".",
    "paths": {
      "*": ["node_modules/*", "src/types/*"]
    }
  },
  "include": ["src/**/*"]
}
